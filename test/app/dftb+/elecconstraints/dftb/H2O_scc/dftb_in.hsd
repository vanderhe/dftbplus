Geometry = GenFormat {
 3  C
  O H
     1    1    0.00000000000  -1.00000000000   0.00000000000
     2    2    0.00000000000   0.00000000000   0.78306400000
     3    2    0.00000000000   0.00000000000  -0.78306400000
}

Hamiltonian = DFTB {

  SCC = Yes
  SCCTolerance = 1.0E-010

  MaxAngularMomentum = {
    O = "p"
    H = "s"
  }

  Filling = Fermi {
    Temperature [Kelvin] = 1.0
  }

  SlaterKosterFiles = Type2FileNames {
    Separator = "-"
    Suffix = ".skf"
    Prefix = "slakos/origin/mio-1-1/"
  }

  StateConstraints {
    Regions = {
      Atoms = {
        Domain = H
        Population = 1.5
      }
      Atoms = {
        Domain = 3
        Population = 1.0
      }
    }
    Optimiser = Rational {}
    ConstrTolerance = 1.0e-10
    MaxConstrIterations = 30
  }

}

Options {
  WriteAutotestTag = Yes
}

Analysis {
  CalculateForces = Yes
}

ParserOptions {
  ParserVersion = 12
}

Parallel {
  UseOmpThreads = Yes
}
