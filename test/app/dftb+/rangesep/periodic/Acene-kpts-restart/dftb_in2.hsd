Geometry = GenFormat {
   <<< Acene_Primitive_Cell.gen
}

Hamiltonian = DFTB {

  SCC = Yes
  ReadInitialCharges = Yes
  MaxSccIterations = 1
  SccTolerance = 1e-08

  Filling = Fermi {
    Temperature [K] = 0.0
  }

  MaxAngularMomentum = {
    C = "p"
    H = "s"
  }

  SlaterKosterFiles = Type2FileNames {
    Prefix = "slakos/origin/ob2-1-1/shift/"
    Separator = "-"
    Suffix = ".skf"
  }

  RangeSeparated = LC {
    Screening = NeighbourBased {
      GammaCutoff = 6.001070
      GSummationCutoff = 28.721100
      Threshold = 1e-100
    }
  }

  KPointsAndWeights = SupercellFolding {
     1   0   0
     0   1   0
     0   0   9
    0.0 0.0 0.0
  }

}

Analysis {
  CalculateForces = No
}

Options {
  WriteAutotestTag = Yes
}

ParserOptions {
    ParserVersion = 11
}

Parallel {
  UseOmpThreads = No
  Groups = 1
}